{
  "name": "dpexelselectron",
  "version": "2.1.0",
  "description": "THIS IS A GUI PROGRAM FOR DOWNLOADING BULK QUANTITY OF IMAGES FORM PEXELS API",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . dPexels --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"dPexels\"",
    "package-linux": "electron-packager . dPexels --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomiVidal99/dPexelsElectron.git"
  },
  "author": "Tom√°s Vidal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TomiVidal99/dPexelsElectron/issues"
  },
  "homepage": "https://github.com/TomiVidal99/dPexelsElectron#readme",
  "dependencies": {
    "axios": "^0.19.1",
    "electron-json-storage": "^4.1.8",
    "pexels-api-wrapper": "^1.1.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.2.0",
    "electron-packager": "^14.2.0"
  }
}
